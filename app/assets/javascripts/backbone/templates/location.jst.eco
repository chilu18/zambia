<div class="row location-row">
  <div class="span4">
    <h2 class="city"><%= @model.get "city" %></h2>
    <h3 class="coffeeshop"><%= @model.get "coffeeshop"%></h3>
    <p class="day-time"><%= @model.get "day" %> @ <%= @model.get "time" %></p>
  </div>
  <div class="span2">
    <p><button class="btn btn-primary btn-large" href="#">Check-in</button></p>
    <p><a class="btn btn-inverse" href="<%= @model.get "map" %>">Map</a></p>
  </div>
</div>
